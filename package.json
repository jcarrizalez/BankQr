  "private": true,
  "keywords": [],
  "author": "Juan Carrizalez <sitgem@gmail.com>",
  "scripts": {
    "android": "react-native run-android",
    "back": "adb shell input keyevent 82",
    "studio": "sh /home/jcarrizalez/Android/Studio/bin/studio.sh",
    "certificado-firebase": "cd android && ./gradlew signingReport && cd ..",
    "start": "rm -rf /tmp/metro-*;react-native start --reset-cache",
    "clean": "cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd ..",
    "ios": "react-native run-ios",
    "key": "keytool -genkey -v -keystore BankQr.keystore -alias BankQr -keyalg RSA -keysize 2048 -validity 10000; mv BankQr.keystore android/app/;",
    "build-debug": "mkdir -p android/app/build/intermediates/res/merged/release/;react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "build-release": "mkdir -p android/app/build/intermediates/res/merged/release/;react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "test": "jest",
    "lint": "eslint .",
    "jetify": "npx jetify"
  },
